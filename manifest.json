{
    "manifest_version": 3,
    "name": "Audio Equalizer",
    "description": "An equalizer for any audio playing in the tab.",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "audioCapture",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  